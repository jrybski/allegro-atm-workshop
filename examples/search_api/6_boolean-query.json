GET /nycpd/accidents/_search
{
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "on_street_name": "CENTRAL AVENUE"
          }
        }
      ],
      "should": [
        {
          "term": {
            "on_street_name-analyzed": "avenue"
          }
        }
      ], 
      "must_not": [
        {
          "range": {
            "number_of_persons_injured": {
              "lte": 1
            }
          }
        }
      ],
      "minimum_number_should_match": 1
    }
  }
}
