GET /nycpd/accidents/_search
{
  "query": {
    "filtered": {
      "query": {
        "match": {
          "borough": "MANHATTAN"
        }
      },
      "filter": {
        "and": {
          "filters": [
            {
              "term": {
                "vehicle_type": "passenger"
              }
            },
            {
              "range": {
                "@timestamp": {
                  "gte": "2015-08-15",
                  "lte": "2015-08-16"
                }
              }
            }
          ]
        }
      }
    }
  }
}
